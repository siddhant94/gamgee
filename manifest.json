{
  "name": "Gamgee",
  "description": "Store all tabs along with groups for a session!",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [ "tabs", "tabGroups" ],
  "background": { "service_worker": "background.js" },
  "action": { "default_popup": "gamgee.html" },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens gamgee.html"
    }
  }
}